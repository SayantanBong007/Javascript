const arr=[0,1,2,3,4,5]
let arr1=[0,2,5,6,7]
console.log(arr);
console.log(arr1);

arr.unshift(-1)
console.log(arr);
arr.shift()
console.log(arr)

const newarr=arr.join()
console.log(newarr)
console.log(typeof newarr)

//Slice and Splice

const ar=[1,2,3,4,5]
console.log(ar)

const ar1=ar.slice(1,3)
console.log("Slice Array",ar1)
console.log("Real Array",ar1)

const ar2=ar.splice(1,3)
console.log("Splice Array",ar2)
console.log("Real Array",ar)